
{
  "version": 2,
  "builds": [
    {
      "src": "server1.2.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",   // All requests to /api/* should be routed to the server
      "dest": "server1.2.js"
    },
    {
      "src": "/(.*)",
      "dest": "/edit.html"
    }
  ]
}

